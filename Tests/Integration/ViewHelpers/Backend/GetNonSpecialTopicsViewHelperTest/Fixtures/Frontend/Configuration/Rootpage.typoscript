module.tx_rkwnewsletter {
	persistence {
		storagePid = {$module.tx_rkwnewsletter.persistence.storagePid}
	}
    view {
        layoutRootPaths {
            0 = EXT:rkw_newsletter/Resources/Private/Layouts/Backend/
            1 = {$module.tx_rkwnewsletter.view.layoutRootPath}
        }
        templateRootPaths {
            0 = EXT:rkw_newsletter/Resources/Private/Templates/Backend/
            1 = {$module.tx_rkwnewsletter.view.templateRootPath}
        }
        partialRootPaths {
            0 = EXT:rkw_newsletter/Resources/Private/Partials/Backend/
            1 = {$module.tx_rkwnewsletter.view.partialRootPath}
        }

        newsletter {

            layoutRootPaths {
                0 = EXT:rkw_newsletter/Resources/Private/Layouts/Newsletter
                1 = {$module.tx_rkwnewsletter.view.newsletter.layoutRootPath}
            }
            templateRootPaths {
                0 = EXT:rkw_newsletter/Resources/Private/Templates/Newsletter
                1 = {$module.tx_rkwnewsletter.view.newsletter.templateRootPath}
            }
            partialRootPaths {
                0 = EXT:rkw_newsletter/Resources/Private/Partials/Newsletter
                1 = {$module.tx_rkwnewsletter.view.newsletter.partialRootPath}
            }
        }
    }

    persistence {
        # cat=module.tx_rkwnewsletter//a; type=string; label=Default storage PID
        storagePid = 9999
    }

    settings < plugin.tx_rkwnewsletter.settings

}

# ========================================

config {
    doctype = html5
    xmlprologue = none

    inlineStyle2TempFile = 0
    removeDefaultJS = 0
    compressJs = 0
    concatenateJs = 0
    compressCss = 0
    concatenateCss = 0

    no_cache = 1
    debug = 0
    xhtml_cleaning = 0
    admPanel = 0
    disableAllHeaderCode = 1
    sendCacheHeaders = 0
    absRefPrefix = /
    contentObjectExceptionHandler = 0

    tx_realurl_enable = 1
}

page = PAGE
page {
    10 = TEXT
    10.value = Hallo Welt!
}